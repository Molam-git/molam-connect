{
  "name": "@molam/brique-61-subscription-analytics",
  "version": "1.0.0",
  "description": "Subscription Analytics & Churn Prevention with SIRA (Industrial ML Platform)",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "worker:analytics": "ts-node src/workers/analyticsGenerator.ts",
    "worker:churn": "ts-node src/workers/churnPredictor.ts",
    "worker:features": "ts-node src/workers/featureIngest.ts",
    "worker:retrain": "ts-node src/workers/retrainOrchestrator.ts",
    "python:train": "python3 src/sira/trainer/train_churn.py",
    "python:scorer": "python3 src/sira/scoring_service.py"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "prom-client": "^15.1.0",
    "node-fetch": "^2.7.0",
    "kafkajs": "^2.2.4",
    "aws-sdk": "^2.1519.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/node-fetch": "^2.6.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  }
}
