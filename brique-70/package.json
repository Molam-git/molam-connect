{
  "name": "@molam/brique-70-marketing",
  "version": "1.0.0",
  "description": "Marketing tools for Molam Connect - promo codes, coupons, subscriptions",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "subscriptions": "tsx src/jobs/subscriptionWorker.ts",
    "test": "jest",
    "migrate": "node -r tsx/register migrations/run.ts",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build"
  },
  "keywords": ["marketing", "promo", "coupons", "subscriptions", "molam"],
  "author": "Molam Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "date-fns": "^3.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.1",
    "cron": "^3.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cron": "^2.4.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.2",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2"
  }
}
