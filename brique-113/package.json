{
  "name": "@molam/sira-inference",
  "version": "1.0.0",
  "description": "SIRA Inference Service - Low-latency ML inference with canary routing",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "docker:build": "docker build -t molam/sira-inference:latest .",
    "k8s:deploy": "kubectl apply -f k8s/"
  },
  "keywords": ["ml", "inference", "canary", "sira", "molam"],
  "author": "Molam Engineering",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.1.0",
    "prom-client": "^15.1.0",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "aws-sdk": "^2.1491.0",
    "winston": "^3.11.0",
    "onnxruntime-node": "^1.16.3",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
