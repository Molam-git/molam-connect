{
  "name": "@molam/brique-83-sira",
  "version": "1.0.0",
  "description": "SIRA Insights - Industrial ML platform for Molam",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "start:consumer": "node dist/sira/consumer.js",
    "start:materializer": "node dist/sira/materializer.js",
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "dev:consumer": "ts-node src/sira/consumer.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "psql -U postgres -d molam_connect -f sql/012_sira_tables.sql",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "keywords": [
    "machine-learning",
    "feature-store",
    "model-registry",
    "inference",
    "mlops",
    "ai",
    "sira"
  ],
  "author": "Molam Platform Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "kafkajs": "^2.2.4",
    "ioredis": "^5.3.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "python": ">=3.9"
  },
  "python": {
    "dependencies": [
      "fastapi>=0.104.0",
      "uvicorn>=0.24.0",
      "lightgbm>=4.1.0",
      "scikit-learn>=1.3.0",
      "pandas>=2.1.0",
      "numpy>=1.24.0",
      "psycopg2-binary>=2.9.9",
      "redis>=5.0.0",
      "boto3>=1.29.0",
      "shap>=0.43.0",
      "prometheus-client>=0.19.0",
      "pydantic>=2.5.0"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
